{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/images\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/middlewares/internal-key/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/middlewares/origin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/movies\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/search\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/services/BonsaiSearch/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}